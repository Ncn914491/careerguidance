{
  "name": "career-guidance-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:all": "node tests/run-all-tests.js",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true npm run build",
    "db:migrate": "node sql/migrate.js",
    "db:migrate-pending-admin": "node scripts/migrate-pending-admin.js",
    "db:verify": "node sql/verify.js",
    "db:verify-roles": "node scripts/verify-role-system.js",
    "db:seed": "echo 'Please run seed.sql manually in Supabase dashboard or use supabase CLI'",
    "db:seed-admin": "node sql/seed-admin.js",
    "db:fix": "node scripts/apply-comprehensive-fix.js",
    "db:fix-step": "node scripts/fix-database-step-by-step.js",
    "db:setup": "node scripts/setup-system.js",
    "db:test": "node scripts/test-system.js",
    "admin:create": "node scripts/create-admin.js",
    "vercel:build": "npm run type-check && npm run test:ci && npm run build",
    "postinstall": "npm run type-check"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@heroicons/react": "^2.2.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.0",
    "dotenv": "^17.2.2",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "swr": "^2.3.6",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "node-mocks-http": "^1.17.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
